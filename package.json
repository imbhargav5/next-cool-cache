{
  "name": "next-cool-cache",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "biome check",
    "lint:fix": "biome check --fix",
    "format": "biome format --write .",
    "check-types": "turbo run check-types",
    "changeset": "changeset",
    "version-packages": "changeset version && node ./scripts/bump-peer-dep-ranges.js",
    "release": "changeset publish"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.28.1",
    "@manypkg/cli": "^0.23.0",
    "@manypkg/get-packages": "^2.2.2",
    "turbo": "^2.6.3",
    "typescript": "5.9.2",
    "ultracite": "6.3.10"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
