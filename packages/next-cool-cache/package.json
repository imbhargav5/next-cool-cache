{
  "name": "next-cool-cache",
  "version": "0.0.2",
  "description": "Type-safe cache tag management for Next.js 16",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules' jest --watch",
    "check-types": "tsc --noEmit",
    "prepack": "rm -f README.md && cp ../../README.md README.md"
  },
  "peerDependencies": {
    "next": ">=16.0.0"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    }
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "5.9.2"
  },
  "keywords": [
    "next.js",
    "cache",
    "typescript",
    "type-safe",
    "cacheTag",
    "revalidateTag"
  ],
  "homepage": "https://next-cool-cache-docs.vercel.app/docs",
  "repository": {
    "type": "git",
    "url": "https://github.com/imbhargav5/next-cool-cache.git",
    "directory": "packages/next-cool-cache"
  },
  "license": "MIT"
}
